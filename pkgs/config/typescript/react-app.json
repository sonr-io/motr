{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./paths.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["vite/client", "@cloudflare/workers-types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["src/worker.ts", "src/workflows", "node_modules", "dist"]
}
