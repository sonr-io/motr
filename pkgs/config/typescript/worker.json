{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./library.json",
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "WebWorker"],
    "types": ["@cloudflare/workers-types"],
    "noEmit": false,
    "emitDeclarationOnly": false,
    "allowImportingTsExtensions": false,
    "verbatimModuleSyntax": false,
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true
  }
}
