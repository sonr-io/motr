{
  "name": "@sonr.io/config",
  "version": "0.1.0",
  "description": "Shared configuration files for Sonr monorepo",
  "type": "module",
  "packageManager": "bun@1.3.0",
  "scripts": {
    "build": "bun scripts/bundle-assets.js"
  },
  "exports": {
    "./typescript/base": "./typescript/base.json",
    "./typescript/react-app": "./typescript/react-app.json",
    "./typescript/react-library": "./typescript/react-library.json",
    "./typescript/library": "./typescript/library.json",
    "./typescript/worker": "./typescript/worker.json",
    "./vite/react-app": "./vite/react-app.ts",
    "./vite/library": "./vite/library.ts",
    "./vite/test": "./vite/test.ts",
    "./tailwind": "./tailwind/index.js",
    "./oxlint": "./.oxlintrc.json"
  },
  "files": [
    "typescript",
    "vite",
    "tailwind",
    "lint",
    ".oxlintrc.json",
    "public",
    "scripts"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "latest",
    "@tanstack/router-plugin": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-react": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "vite": ">=5.0.0"
  }
}
