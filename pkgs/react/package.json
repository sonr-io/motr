{
  "name": "@sonr.io/react",
  "version": "0.0.1",
  "description": "Modern React hooks for Sonr enclave operations and cryptographic vault management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "providers": [
        "./dist/providers/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "bunx oxlint@latest .",
    "format": "bunx oxfmt@latest .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check . && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo .vite node_modules/.vite",
    "clean:all": "rm -rf dist node_modules *.tsbuildinfo .vite",
    "prepublishOnly": "bun run clean && bun run build && bun run test"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "dependencies": {
    "@sonr.io/enclave": "workspace:*",
    "@sonr.io/sdk": "workspace:*",
    "@tanstack/query-core": "^5.90.3",
    "@tanstack/react-query": "^5.90.3"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/ui": "^3.2.4",
    "fake-indexeddb": "^6.2.3",
    "jsdom": "^27.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react",
    "hooks",
    "sonr",
    "enclave",
    "vault",
    "webauthn",
    "cryptography",
    "wasm",
    "state-management",
    "authentication",
    "mpc",
    "ucan",
    "signing",
    "transactions",
    "typescript"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
