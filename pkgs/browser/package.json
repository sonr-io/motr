{
  "name": "@sonr.io/browser",
  "version": "0.0.1",
  "description": "Framework-agnostic browser client with Web Worker WASM integration for Sonr ecosystem",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./workers": {
      "types": "./dist/workers/index.d.ts",
      "import": "./dist/workers/index.js"
    },
    "./workers/enclave": {
      "types": "./dist/workers/enclave-worker.d.ts",
      "import": "./dist/workers/enclave-worker.js"
    },
    "./workers/vault": {
      "types": "./dist/workers/vault-worker.d.ts",
      "import": "./dist/workers/vault-worker.js"
    },
    "./web-apis": {
      "types": "./dist/web-apis/index.d.ts",
      "import": "./dist/web-apis/index.js"
    },
    "./elements": {
      "types": "./dist/web-apis/elements/index.d.ts",
      "import": "./dist/web-apis/elements/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./vite-plugin": {
      "types": "./dist/plugin/vite-plugin.d.ts",
      "import": "./dist/plugin/vite-plugin.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["./dist/index.d.ts"],
      "client": ["./dist/client/index.d.ts"],
      "workers": ["./dist/workers/index.d.ts"],
      "workers/enclave": ["./dist/workers/enclave-worker.d.ts"],
      "workers/vault": ["./dist/workers/vault-worker.d.ts"],
      "web-apis": ["./dist/web-apis/index.d.ts"],
      "elements": ["./dist/web-apis/elements/index.d.ts"],
      "storage": ["./dist/storage/index.d.ts"],
      "vite-plugin": ["./dist/plugin/vite-plugin.d.ts"],
      "utils": ["./dist/utils/index.d.ts"]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc-alias && vite build",
    "build:watch": "tsc --watch",
    "dev": "vite",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "bunx oxlint@latest .",
    "format": "bunx oxfmt@latest .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check . && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo .vite node_modules/.vite",
    "clean:all": "rm -rf dist node_modules *.tsbuildinfo .vite",
    "prepublishOnly": "bun run clean && bun run build && bun run test"
  },
  "peerDependencies": {
    "@sonr.io/enclave": "workspace:*",
    "@sonr.io/sdk": "workspace:*",
    "@sonr.io/vault": "workspace:*"
  },
  "dependencies": {
    "@sonr.io/enclave": "workspace:*",
    "@sonr.io/sdk": "workspace:*",
    "@sonr.io/vault": "workspace:*",
    "@tanstack/query-core": "^5.90.3",
    "dexie": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@types/dom-view-transitions": "^1.0.5",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "fake-indexeddb": "^6.2.3",
    "happy-dom": "^15.11.7",
    "playwright": "^1.56.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "browser",
    "web-worker",
    "wasm",
    "webassembly",
    "custom-elements",
    "web-components",
    "framework-agnostic",
    "vite-plugin",
    "sonr",
    "blockchain",
    "cryptography",
    "webauthn",
    "vault",
    "enclave",
    "esm",
    "typescript",
    "indexeddb",
    "service-worker",
    "pwa"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
