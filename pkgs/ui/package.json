{
  "name": "@sonr.io/ui",
  "version": "0.0.10",
  "private": true,
  "packageManager": "bun@1.3.0",
  "main": "src/components/index.ts",
  "types": "src/components/index.ts",
  "exports": {
    ".": "./src/components/index.ts",
    "./components": "./src/components/index.ts",
    "./components/*": "./src/components/*.tsx",
    "./components/backgrounds": "./src/components/backgrounds/*.tsx",
    "./components/ui/*": "./src/components/ui/*.tsx",
    "./lib/utils": "./src/lib/utils.ts",
    "./lib/*": "./src/lib/*.ts",
    "./hooks": "./src/hooks/index.ts",
    "./hooks/*": "./src/hooks/*.ts",
    "./emails": "./src/emails/index.ts",
    "./emails/*": "./src/emails/*.tsx",
    "./styles/*": "./src/styles/*",
    "./vite-plugin": "./src/vite-plugin.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate:icons": "bun scripts/generate-icons.ts",
    "build": "bun run generate:icons && tsc && tsc-alias",
    "build:ui": "tsc --noEmit",
    "dev": "tsc --noEmit --watch",
    "clean": "rm -rf dist *.tsbuildinfo .vite node_modules/.vite src/components/icons",
    "test": "echo 'No tests defined for @sonr.io/ui'",
    "lint": "bunx oxlint@latest .",
    "format": "bunx oxfmt@latest .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check .",
    "typecheck": "tsc --noEmit",
    "bump": "devbox run bump",
    "release": "devbox run release",
    "publish": "devbox run publish"
  },
