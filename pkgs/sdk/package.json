{
  "name": "@sonr.io/sdk",
  "version": "0.0.12",
  "description": "Sonr blockchain client library with RPC, wallet, and auth",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./client/auth": {
      "types": "./dist/client/auth/index.d.ts",
      "import": "./dist/client/auth/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet/index.d.ts",
      "import": "./dist/wallet/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js"
    },
    "./vite-plugin": {
      "types": "./dist/plugin/vite-plugin.d.ts",
      "import": "./dist/plugin/vite-plugin.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "bun run gen && tsc && tsc-alias",
    "clean": "rimraf dist *.tsbuildinfo",
    "gen": "bun scripts/gen-protobufs.mjs && bun scripts/gen-registry.mjs",
    "test": "vitest run",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "bump": "devbox run bump",
    "release": "devbox run release",
    "publish": "devbox run publish"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@noble/hashes": "^2.0.0",
    "@noble/secp256k1": "^3.0.0",
    "@scure/base": "^2.0.0",
    "@scure/bip32": "^2.0.0",
    "@scure/bip39": "^2.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@extism/extism": "2.0.0-rc13",
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^3.0.0",
    "@scure/base": "^2.0.0",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@simplewebauthn/browser": "^13.2.2",
    "@sonr.io/enclave": "workspace:*",
    "@sonr.io/vault": "workspace:*",
    "@tanstack/query-core": "^5.90.3",
    "dexie": "^4.2.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.58.0",
    "@bufbuild/protobuf": "^1.10.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@bufbuild/protoplugin": "^1.10.0",
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^3.0.0",
    "@scure/base": "^2.0.0",
    "@scure/bip32": "^2.0.1",
    "@scure/bip39": "^2.0.1",
    "@simplewebauthn/types": "^12.0.0",
    "@testing-library/react": "^16.3.0",
    "@types/lodash-es": "^4.17.12",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "fake-indexeddb": "^6.2.3",
    "glob": "^11.0.3",
    "jsdom": "^27.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "lodash-es": "^4.17.21",
    "playwright": "^1.56.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@helia/strings": "^5.0.1",
    "@helia/unixfs": "^6.0.1",
    "@helia/verified-fetch": "^3.2.3",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.0",
    "@keplr-wallet/types": "^0.12.280",
    "@libp2p/webrtc": "^6.0.6",
    "@libp2p/websockets": "^10.0.6",
    "@walletconnect/legacy-client": "^2.0.0",
    "@walletconnect/legacy-types": "^2.0.0",
    "@walletconnect/sign-client": "^2.22.3",
    "helia": "^6.0.1",
    "multiformats": "^13.4.1",
    "uint8arrays": "^5.1.0"
  }
}
