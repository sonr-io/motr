{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/main/.schema/devbox.schema.json",
  "packages": ["bun@1.3.0", "wrangler@latest", "gh@latest"],
  "env": {},
  "shell": {
    "init_hook": ["gh extension install yuler/gh-download", "bun install"],
    "scripts": {
      "build": ["bun run build"],
      "bump": ["cz --no-raise 6,21 bump --yes --increment PATCH"]
    }
  }
}
