{
  "name": "@sonr.io/motr",
  "version": "0.1.0",
  "private": true,
  "description": "Motor - Multi-purpose WebAssembly service for secure cryptographic operations and payment processing",
  "author": "Sonr Team",
  "license": "MIT",
  "homepage": "https://github.com/sonr-io/motr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonr-io/motr.git"
  },
  "bugs": {
    "url": "https://github.com/sonr-io/motr/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.3.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "pkgs/*",
    "x/*"
  ],
  "packageManager": "bun@1.3.0",
  "trustedDependencies": [
    "@bufbuild/buf",
    "@bufbuild/protobuf",
    "@bufbuild/protoc-gen-es",
    "@extism/extism",
    "esbuild",
    "protobufjs",
    "sharp",
    "@resvg/resvg-js",
    "@parcel/watcher"
  ],
  "scripts": {
    "_README": "Use 'bun run <script>' not 'bun <script>'. Run './index.ts --help' for orchestrator docs.",
    "build": "VERBOSE=1 MODE=build bun run index.ts",
    "build:apps": "WORKSPACES=@sonr.io/auth,@sonr.io/console,@sonr.io/profile,@sonr.io/search MODE=build bun run index.ts",
    "build:libs": "WORKSPACES=@sonr.io/vault,@sonr.io/enclave MODE=build bun run index.ts",
    "build:pkgs": "WORKSPACES=@sonr.io/sdk,@sonr.io/ui,@sonr.io/react,@sonr.io/config MODE=build bun run index.ts",
    "build:force": "MODE=clean bun run index.ts && MODE=build bun run index.ts",
    "build:verbose": "MODE=build VERBOSE=1 bun run index.ts",
    "test": "MODE=test bun run index.ts",
    "test:all": "MODE=test bun run index.ts",
    "test:watch": "vitest watch",
    "dev": "MODE=build bun run index.ts && cd x/worker && wrangler dev",
    "dev:all": "MODE=dev bun run index.ts",
    "dev:worker": "cd x/worker && wrangler dev",
    "lint": "MODE=lint bun run index.ts",
    "lint:fix": "MODE=lint bun run index.ts",
    "format": "MODE=format bun run index.ts",
    "check": "MODE=check bun run index.ts",
    "typecheck": "MODE=typecheck bun run index.ts",
    "clean": "MODE=clean bun run index.ts && rm -rf node_modules/.cache",
    "clean:cache": "rm -rf node_modules/.cache public",
    "clean:all": "MODE=clean bun run index.ts && rm -rf node_modules public",
    "generate": "cd pkgs/sdk && bun run gen",
    "deploy": "MODE=build bun run index.ts && cd x/worker && wrangler deploy",
    "deploy:staging": "MODE=build bun run index.ts && cd x/worker && wrangler deploy --env staging",
    "deploy:production": "MODE=build bun run index.ts && cd x/worker && wrangler deploy --env production",
    "preview": "MODE=build bun run index.ts && cd x/worker && wrangler dev --remote",
    "logs": "cd x/worker && wrangler tail --format pretty",
    "kv:populate": "cd apps/auth && bun run kv:populate",
    "workspace:install": "bun install",
    "workspace:update": "bun update --latest",
    "workspace:list": "bun pm ls --all",
    "workspace:prune": "bun pm cache rm"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@bufbuild/protoc-gen-es": "1.10.0",
    "@bufbuild/protoplugin": "2.10.0",
    "@cloudflare/vite-plugin": "^1.13.15",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@types/node": "^24.9.1",
    "@vitest/ui": "^4.0.1",
    "happy-dom": "^20.0.8",
    "oxlint": "^1.24.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1",
    "wrangler": "^4.44.0"
  },
  "overrides": {
    "esbuild": "0.20.0",
    "vite": "5.4.20",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@noble/hashes": "2.0.0",
    "@noble/secp256k1": "2.1.0",
    "@bufbuild/protobuf": "1.10.0",
    "@bufbuild/protoc-gen-es": "1.10.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.10.0",
    "@emnapi/wasi-threads": "^1.1.0",
    "@noble/hashes": "2.0.0",
    "@noble/secp256k1": "2.1.0",
    "glob": "^11.0.3",
    "hono": "^4.10.2",
    "lottie-react": "^2.4.1",
    "motion": "^12.23.24",
    "sonner": "^2.0.7",
    "swagger2openapi": "^7.0.8",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  }
}
