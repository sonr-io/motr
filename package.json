{
  "name": "@sonr.io/motr",
  "version": "0.1.0",
  "private": true,
  "description": "Motor - Multi-purpose WebAssembly service for secure cryptographic operations and payment processing",
  "author": "Sonr Team",
  "license": "MIT",
  "homepage": "https://github.com/sonr-io/motr",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonr-io/motr.git"
  },
  "bugs": {
    "url": "https://github.com/sonr-io/motr/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.3.0"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "pkgs/*"
  ],
  "packageManager": "bun@1.3.0",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "cd apps/frontend && bun run dev",
     "lint": "oxlint",
     "format": "oxlint --fix",
     "check": "oxlint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules bun.lock",
    "generate": "bun --filter '@sonr.io/sdk' run gen",
    "deploy": "turbo run deploy"
  },
   "devDependencies": {
     "@types/node": "^24.7.2",
     "@vitest/ui": "^3.2.4",
     "happy-dom": "^20.0.0",
     "turbo": "^2.5.8",
     "typescript": "^5.9.3",
     "vitest": "^3.2.4",
     "wrangler": "^4.42.2"
   },
  "overrides": {
    "esbuild": "0.20.0",
    "vite": "5.4.20",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "swagger2openapi": "^7.0.8",
    "yaml": "^2.8.1"
  }
}
