{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "envMode": "loose",
  "daemon": true,
  "globalDependencies": [
    "**/.env.*local",
    ".env",
    "tsconfig.json",
    "bunfig.toml",
    "bun.lockb"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL_ENV",
    "VERCEL_URL",
    "NEXT_PUBLIC_*",
    "VITE_*",
    "CLOUDFLARE_*",
    "WRANGLER_*",
    "BUN_*"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "out/**",
        "build/**",
        "lib/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.{ts,tsx,js,jsx}",
        "!**/*.spec.{ts,tsx,js,jsx}",
        "!**/__tests__/**",
        "!**/coverage/**"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": ["BUN_*"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["BUN_*", "BROWSER", "BROWSER_ARGS"]
    },
    "@sonr.io/sdk#build": {
      "dependsOn": [],
      "outputs": ["dist/**", "*.tsbuildinfo"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "scripts/**",
        "!scripts/**/*.test.ts"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": ["BUN_*", "GITHUB_TOKEN"]
    },
    "@sonr.io/enclave#build": {
      "dependsOn": ["@sonr.io/sdk#build"],
      "outputs": ["dist/**", "*.tsbuildinfo"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        "go.mod",
        "go.sum",
        "!**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "GOOS", "GOARCH", "CGO_ENABLED"]
    },
    "@sonr.io/vault#build": {
      "dependsOn": ["@sonr.io/sdk#build"],
      "outputs": ["dist/**", "*.tsbuildinfo"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        "go.mod",
        "go.sum",
        "wasm_exec.js",
        "vite.config.cloudflare.ts",
        "!**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "GOOS", "GOARCH", "CGO_ENABLED"]
    },
    "@sonr.io/ui#build": {
      "dependsOn": ["@sonr.io/sdk#build"],
      "outputs": ["dist/**", "*.tsbuildinfo", "src/components/icons/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "assets/**/*.svg",
        "assets/**/*.json",
        "scripts/**"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": ["BUN_*"]
    },
    "@sonr.io/react#build": {
      "dependsOn": ["@sonr.io/ui#build"],
      "outputs": ["dist/**", "*.tsbuildinfo"],
      "inputs": ["$TURBO_DEFAULT$"],
      "cache": true,
      "passThroughEnv": ["BUN_*"]
    },
    "@sonr.io/auth#build": {
      "dependsOn": ["@sonr.io/react#build", "@sonr.io/vault#build"],
      "outputs": ["dist/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "public/**",
        "tailwind.config.ts"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "VITE_*"]
    },
    "@sonr.io/console#build": {
      "dependsOn": ["@sonr.io/react#build", "@sonr.io/enclave#build"],
      "outputs": ["dist/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "public/**",
        "tailwind.config.ts"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "VITE_*"]
    },
    "@sonr.io/profile#build": {
      "dependsOn": ["@sonr.io/react#build", "@sonr.io/enclave#build"],
      "outputs": ["dist/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "public/**",
        "tailwind.config.ts"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "VITE_*"]
    },
    "@sonr.io/search#build": {
      "dependsOn": ["@sonr.io/react#build", "@sonr.io/enclave#build"],
      "outputs": ["dist/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "public/**",
        "tailwind.config.ts"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "VITE_*"]
    },
    "@sonr.io/worker#build": {
      "dependsOn": [
        "@sonr.io/auth#build",
        "@sonr.io/console#build",
        "@sonr.io/profile#build",
        "@sonr.io/search#build"
      ],
      "outputs": ["tsconfig.tsbuildinfo"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "../auth/dist/**",
        "../console/dist/**",
        "../profile/dist/**",
        "../search/dist/**",
        "wrangler.toml"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": ["BUN_*"]
    },
    "format": {
      "outputs": [],
      "inputs": ["$TURBO_DEFAULT$", "**/*.go"],
      "cache": false,
      "passThroughEnv": ["BUN_*"]
    },
    "lint": {
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        ".oxlintrc.json",
        ".golangci.yml"
      ],
      "cache": false,
      "passThroughEnv": ["BUN_*"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", ".vitest/**"],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": ["BUN_*", "CI"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": ["*.tsbuildinfo"],
      "inputs": ["$TURBO_DEFAULT$"],
      "cache": true,
      "passThroughEnv": ["BUN_*"]
    },
    "check": {
      "dependsOn": ["typecheck", "lint"],
      "outputs": [],
      "cache": false,
      "passThroughEnv": ["BUN_*"]
    },
    "clean": {
      "cache": false
    },
    "@sonr.io/worker#deploy": {
      "dependsOn": ["@sonr.io/worker#build"],
      "outputs": [],
      "cache": false,
      "inputs": [
        "src/**",
        "wrangler.toml",
        "../auth/dist/**",
        "../console/dist/**",
        "../profile/dist/**",
        "../search/dist/**"
      ],
      "passThroughEnv": ["BUN_*", "CLOUDFLARE_*", "WRANGLER_*"]
    },
    "deploy": {
      "dependsOn": ["@sonr.io/worker#deploy"],
      "outputs": [],
      "cache": false,
      "passThroughEnv": ["BUN_*", "CLOUDFLARE_*", "WRANGLER_*"]
    }
  }
}
