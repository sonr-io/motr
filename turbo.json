{
  "$schema": "https://turbo.build/schema.json",
  "envMode": "loose",
  "daemon": false,
  "globalDependencies": [
    "**/.env.*local",
    ".env",
    "tsconfig.json",
    "bunfig.toml",
    "bun.lockb"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL_ENV",
    "VERCEL_URL",
    "NEXT_PUBLIC_*",
    "VITE_*",
    "CLOUDFLARE_*",
    "WRANGLER_*",
    "BUN_*"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "out/**",
        "build/**",
        "lib/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!**/*.md",
        "!**/*.test.{ts,tsx,js,jsx}",
        "!**/*.spec.{ts,tsx,js,jsx}",
        "!**/__tests__/**",
        "!**/coverage/**"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": [
        "BUN_*",
        "BROWSER",
        "BROWSER_ARGS"
      ]
    },
    "@sonr.io/sdk#build": {
      "dependsOn": [],
      "outputs": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "scripts/**",
        "!scripts/**/*.test.ts"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": [
        "BUN_*",
        "GITHUB_TOKEN"
      ]
    },
    "@sonr.io/enclave#build": {
      "dependsOn": [
        "@sonr.io/sdk#build"
      ],
      "outputs": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        "go.mod",
        "go.sum",
        "!**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": [
        "BUN_*",
        "GOOS",
        "GOARCH",
        "CGO_ENABLED"
      ]
    },
    "@sonr.io/vault#build": {
      "dependsOn": [
        "@sonr.io/sdk#build"
      ],
      "outputs": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        "go.mod",
        "go.sum",
        "wasm_exec.js",
        "vite.config.cloudflare.ts",
        "!**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": [
        "BUN_*",
        "GOOS",
        "GOARCH",
        "CGO_ENABLED"
      ]
    },
    "@sonr.io/ui#build": {
      "dependsOn": [],
      "outputs": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "src/**/*.svelte",
        "src/**/*.ts",
        "!src/**/*.test.ts"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "@sonr.io/web#build": {
      "dependsOn": [
        "@sonr.io/ui#build",
        "@sonr.io/sdk#build"
      ],
      "outputs": [
        ".svelte-kit/**",
        "!.svelte-kit/cache/**",
        "build/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "src/**/*.svelte",
        "src/**/*.ts",
        "!src/**/*.test.ts",
        "!src/**/*.spec.ts",
        "static/**"
      ],
      "cache": true,
      "persistent": false,
      "passThroughEnv": [
        "BUN_*",
        "PUBLIC_*"
      ]
    },
    "format": {
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go"
      ],
      "cache": false,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "lint": {
      "dependsOn": [],
      "outputs": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.go",
        ".oxlintrc.json",
        ".golangci.yml"
      ],
      "cache": false,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**",
        ".vitest/**"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/*_test.go"
      ],
      "cache": true,
      "passThroughEnv": [
        "BUN_*",
        "CI"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "*.tsbuildinfo"
      ],
      "inputs": [
        "$TURBO_DEFAULT$"
      ],
      "cache": true,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "check": {
      "dependsOn": [
        "typecheck",
        "lint"
      ],
      "outputs": [],
      "cache": false,
      "passThroughEnv": [
        "BUN_*"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#api:deploy": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [],
      "cache": false,
      "inputs": [
        "apps/api/src/**",
        "apps/api/wrangler.toml"
      ],
      "passThroughEnv": [
        "BUN_*",
        "CLOUDFLARE_*",
        "WRANGLER_*",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_API_TOKEN"
      ]
    }
  }
}
