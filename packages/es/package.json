{
  "name": "@sonr.io/es",
  "version": "0.0.12",
  "description": "Sonr blockchain client library with RPC, wallet, and auth",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
     ".": {
       "types": "./dist/index.d.ts",
       "import": "./dist/index.js",
       "default": "./dist/index.js"
     },
     "./client": {
       "types": "./dist/client/index.d.ts",
       "import": "./dist/client/index.js"
     },
     "./client/auth": {
       "types": "./dist/client/auth/index.d.ts",
       "import": "./dist/client/auth/index.js"
     },
     "./wallet": {
       "types": "./dist/wallet/index.d.ts",
       "import": "./dist/wallet/index.js"
     },
     "./vite": {
       "types": "./dist/vite/index.d.ts",
       "import": "./dist/vite/index.js"
     },
     "./vite-plugin": {
       "types": "./dist/plugin/vite-plugin.d.ts",
       "import": "./dist/plugin/vite-plugin.js"
     }
   },
  "scripts": {
    "dev": "tsc --watch",
    "build": "pnpm run gen && tsc && tsc-alias",
    "clean": "rimraf dist *.tsbuildinfo",
    "gen": "node scripts/gen-protobufs.mjs && node scripts/gen-registry.mjs",
    "test": "vitest run",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@bufbuild/protobuf": "^1.2.0",
    "@noble/hashes": "^1.3.0",
    "@noble/secp256k1": "^2.0.0",
    "@scure/base": "^1.1.0",
    "@scure/bip32": "^1.3.0",
    "@scure/bip39": "^1.2.0"
  },
   "dependencies": {
     "@extism/extism": "2.0.0-rc13",
     "@simplewebauthn/browser": "^9.0.1",
     "@sonr.io/enclave": "workspace:*",
     "@tanstack/query-core": "^5.0.0",
     "dexie": "^4.0.1"
   },
   "devDependencies": {
     "@biomejs/biome": "^2.2.4",
     "@bufbuild/buf": "^1.18.0",
     "@bufbuild/protobuf": "^1.2.0",
     "@bufbuild/protoc-gen-es": "^1.2.0",
     "@bufbuild/protoplugin": "^1.2.0",
     "@noble/hashes": "^1.3.2",
     "@noble/secp256k1": "^2.0.0",
     "@scure/base": "^1.1.3",
     "@scure/bip32": "^1.3.2",
     "@scure/bip39": "^1.2.1",
     "@simplewebauthn/types": "^12.0.0",
     "@testing-library/react": "^14.0.0",
     "@types/lodash-es": "^4.17.12",
     "@vitest/ui": "^3.2.4",
     "concurrently": "^9.1.2",
     "fake-indexeddb": "^6.0.0",
     "glob": "^11.0.0",
     "jsdom": "^27.0.0",
     "json-schema-to-typescript": "^13.1.1",
     "lodash-es": "^4.17.21",
     "rimraf": "^6.0.1",
     "tsc-alias": "^1.8.10",
     "typescript": "^5.9.2",
     "vite": "^5.4.20",
     "vitest": "^3.2.4"
   },
  "optionalDependencies": {
    "@helia/strings": "^3.0.0",
    "@helia/unixfs": "^3.0.0",
    "@helia/verified-fetch": "^1.0.0",
    "@chainsafe/libp2p-noise": "^15.0.0",
    "@chainsafe/libp2p-yamux": "^6.0.0",
    "@keplr-wallet/types": "^0.11.62",
    "@libp2p/webrtc": "^4.0.0",
    "@libp2p/websockets": "^8.0.0",
    "@walletconnect/legacy-client": "^2.0.0",
    "@walletconnect/legacy-types": "^2.0.0",
    "@walletconnect/sign-client": "^2.8.6",
    "helia": "^4.0.0",
    "multiformats": "^13.0.0",
    "uint8arrays": "^5.0.0"
  }
}
