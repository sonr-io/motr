{
  "name": "@sonr.io/vault",
  "version": "0.0.1",
  "description": "WebAssembly-based HTTP server for Motor Payment Gateway and OIDC services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "exports": {
    ".": "./dist/index.js",
    "./dist/vault.wasm": "./dist/vault.wasm",
    "./loader": "./dist/loader.js",
    "./vite-plugin": "./dist/vite-plugin-vault.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "devbox run build && tsc && tsc-alias",
    "watch": "tsc --watch",
    "test": "bun run test:go && vitest run",
    "test:watch": "vitest",
    "test:go": "go test -v ./... || echo 'No Go tests found in vault'",
    "lint": "oxlint && go fmt ./...",
    "format": "(oxlint --fix || true) && go fmt ./...",
    "check": "oxlint",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "wasm",
    "http-server",
    "service-worker",
    "payment-gateway",
    "oidc",
    "sonr",
    "motor"
  ],
  "author": "diDAO",
  "license": "MIT",
  "private": true
}
