{
  "name": "@sonr.io/vault",
  "version": "0.1.0",
  "description": "WebAssembly-based HTTP server for Motor Payment Gateway and OIDC services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "exports": {
    ".": "./dist/index.js",
    "./dist/vault.wasm": "./dist/vault.wasm",
    "./loader": "./dist/loader.js",
    "./vite-plugin": "./dist/vite-plugin-vault.js"
  },
  "files": [
    "dist"
  ],
    "scripts": {
      "build": "bun run build:wasm && tsc",
      "build:wasm": "mkdir -p dist && GOOS=js GOARCH=wasm go build -ldflags='-s -w' -trimpath -o dist/vault.wasm . && rm -f dist/wasm_exec.js && cp wasm_exec.js dist/ && chmod 644 dist/wasm_exec.js",
      "watch": "tsc --watch",
      "test": "bun run test:go && vitest run",
      "test:watch": "vitest",
      "test:go": "go test -v ./... || echo 'No Go tests found in vault'",
      "lint": "go fmt ./...",
      "clean": "rm -rf dist *.tsbuildinfo"
    },
   "devDependencies": {
     "typescript": "^5.9.3",
     "vite": "^5.4.20",
     "vitest": "^2.0.0",
     "@vitest/coverage-v8": "^2.0.0"
   },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "wasm",
    "http-server",
    "service-worker",
    "payment-gateway",
    "oidc",
    "sonr",
    "motor"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
