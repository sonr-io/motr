{
  "name": "@sonr.io/enclave",
  "version": "0.0.1",
  "description": "Modern WebAssembly-based cryptographic vault with Web Worker integration for Sonr ecosystem",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./worker-client": {
      "types": "./dist/worker-client.d.ts",
      "import": "./dist/worker-client.js"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "import": "./dist/worker.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "import": "./dist/storage.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin-enclave.d.ts",
      "import": "./dist/vite-plugin-enclave.js"
    },
    "./cloudflare": {
      "types": "./dist/cloudflare/index.d.ts",
      "import": "./dist/cloudflare/index.js"
    },
    "./cloudflare/durable": {
      "types": "./dist/cloudflare/durable/index.d.ts",
      "import": "./dist/cloudflare/durable/index.js"
    },
    "./cloudflare/worker": {
      "types": "./dist/cloudflare/worker.d.ts",
      "import": "./dist/cloudflare/worker.js"
    },
    "./enclave.wasm": "./dist/enclave.wasm",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/client.d.ts"
      ],
      "worker-client": [
        "./dist/worker-client.d.ts"
      ],
      "worker": [
        "./dist/worker.d.ts"
      ],
      "loader": [
        "./dist/loader.d.ts"
      ],
      "storage": [
        "./dist/storage.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ],
      "vite-plugin": [
        "./dist/vite-plugin-enclave.d.ts"
      ],
      "cloudflare": [
        "./dist/cloudflare/index.d.ts"
      ],
      "cloudflare/durable": [
        "./dist/cloudflare/durable/index.d.ts"
      ],
      "cloudflare/worker": [
        "./dist/cloudflare/worker.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "devbox run wasm && vite build && rm -f *.tsbuildinfo && bunx tsc && tsc-alias && cp dist/wasm_exec.js dist/wasm_exec.js 2>/dev/null || true",
    "build:watch": "vite build --watch",
    "build:wasm": "devbox run wasm",
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "preview": "vite preview",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "bunx oxlint@latest . && export GOROOT=$(readlink -f $(which go) | sed 's|/bin/go||' | sed 's|/share/go||')/share/go && ../../.devbox/nix/profile/default/bin/golangci-lint run --config ../../.golangci.yml .",
    "lint:fix": "bunx oxlint@latest --fix . && export GOROOT=$(readlink -f $(which go) | sed 's|/bin/go||' | sed 's|/share/go||')/share/go && ../../.devbox/nix/profile/default/bin/golangci-lint run --fix --config ../../.golangci.yml .",
    "format": "bunx oxfmt@latest . && ../../.devbox/nix/profile/default/bin/gofumpt -w .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check . && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo .vite node_modules/.vite",
    "clean:all": "rm -rf dist node_modules *.tsbuildinfo .vite",
    "prepublishOnly": "bun run clean && bun run build && bun run test",
    "bump": "devbox run bump",
    "release": "devbox run release",
    "publish": "devbox run publish"
  },
  "dependencies": {
    "@extism/extism": "2.0.0-rc13",
    "dexie": "^4.2.1"
  },
  "optionalDependencies": {
    "helia": "^6.0.1",
    "@helia/strings": "^5.0.1",
    "@helia/unixfs": "^6.0.1",
    "@helia/verified-fetch": "^3.2.3",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.0",
    "@libp2p/webrtc": "^6.0.6",
    "@libp2p/websockets": "^10.0.6",
    "multiformats": "^13.4.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250110.0",
    "wrangler": "^4.9.0",
    "fake-indexeddb": "^6.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "web-worker",
    "worker",
    "extism",
    "vite-plugin",
    "cryptography",
    "mpc",
    "ucan",
    "vault",
    "indexeddb",
    "dexie",
    "ipfs",
    "signing",
    "verification",
    "background-processing",
    "web-standards",
    "modern-web",
    "sonr",
    "blockchain",
    "typescript"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
