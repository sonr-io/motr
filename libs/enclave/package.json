{
  "name": "@sonr.io/enclave",
  "version": "0.1.0",
  "description": "WebAssembly-based vault plugin for Sonr blockchain with Extism and Vite integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "exports": {
    ".": "./dist/index.js",
    "./dist/enclave.wasm": "./dist/enclave.wasm",
    "./loader": "./dist/loader.js",
    "./vite-plugin": "./dist/vite-plugin-enclave.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "devbox run build && tsc --declaration --emitDeclarationOnly",
    "watch": "tsc --watch",
    "test": "go test ./... || echo 'No Go tests found in enclave'",
    "lint": "go fmt ./...",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "wasm",
    "extism",
    "vite",
    "sonr",
    "blockchain",
    "vault"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
