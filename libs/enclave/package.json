{
  "name": "@sonr.io/enclave",
  "version": "0.0.1",
  "description": "WebAssembly-based vault plugin for Sonr blockchain with Extism and Vite integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "bun@1.3.0",
  "exports": {
    ".": "./dist/index.js",
    "./dist/enclave.wasm": "./dist/enclave.wasm",
    "./loader": "./dist/loader.js",
    "./vite-plugin": "./dist/vite-plugin-enclave.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "devbox run wasm && tsc && tsc-alias",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint && go fmt ./...",
    "format": "(oxlint --fix || true) && go fmt ./...",
    "check": "oxlint",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo",
    "bump": "devbox run bump",
    "release": "devbox run release",
    "publish": "devbox run publish"
  },
  "dependencies": {
    "@extism/extism": "2.0.0-rc13",
    "dexie": "^4.2.1"
  },
  "optionalDependencies": {
    "helia": "^6.0.1",
    "@helia/strings": "^5.0.1",
    "@helia/unixfs": "^6.0.1",
    "@helia/verified-fetch": "^3.2.3",
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@chainsafe/libp2p-yamux": "^8.0.0",
    "@libp2p/webrtc": "^6.0.6",
    "@libp2p/websockets": "^10.0.6",
    "multiformats": "^13.4.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "wrangler": "^4.9.0",
    "fake-indexeddb": "^6.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "wasm",
    "extism",
    "vite",
    "sonr",
    "blockchain",
    "vault"
  ],
  "author": "Sonr Labs",
  "license": "MIT",
  "private": true
}
