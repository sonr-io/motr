{
  "name": "@sonr.io/auth",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "predev": "bun run copy:vault",
    "dev": "wrangler dev",
    "prebuild": "bun run copy:vault",
    "build": "bunx --bun vite build",
    "typecheck": "tsc",
    "copy:vault": "cp ../../libs/vault/dist/sw.js public/sw.js && cp ../../libs/vault/dist/vault.wasm public/vault.wasm && cp ../../libs/vault/dist/wasm_exec.js public/wasm_exec.js",
    "preview": "wrangler dev --remote",
    "test": "vitest run --passWithNoTests",
    "format": "bunx oxfmt@latest .",
    "lint": "bunx oxlint@latest .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check .",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "logs": "wrangler tail --format pretty",
    "kv:populate": "bun scripts/populate-kv.ts",
    "clean": "rm -rf dist *.tsbuildinfo .vite node_modules/.vite",
    "clean:all": "rm -rf dist node_modules *.tsbuildinfo .vite"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@sonr.io/react": "workspace:*",
    "@sonr.io/sdk": "workspace:*",
    "@sonr.io/ui": "workspace:*",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.3",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-devtools": "^1.132.51",
    "@tanstack/router-plugin": "^1.132.56",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "cnfg": "workspace:*",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "wrangler": "^4.43.0"
  }
}
