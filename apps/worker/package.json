{
  "name": "@sonr.io/worker",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.3.0",
  "description": "Central Cloudflare Worker orchestrator for Motr frontends",
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "typecheck": "tsc",
    "preview": "wrangler dev --remote",
    "test": "vitest run --passWithNoTests",
    "format": "bunx oxfmt@latest .",
    "lint": "bunx oxlint@latest .",
    "check": "bunx oxlint@latest . && bunx oxfmt@latest --check .",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "logs": "wrangler tail --format pretty",
    "clean": "rm -rf *.tsbuildinfo .wrangler"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20251011.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.43.0"
  }
}
